[
  {
    "label": "Workspace 1",
    "pattern": "^(?=.*\\bworkspace\\b)(?=.*\\b(one|1)\\b).*$",
    "argv": [
      "hyprctl",
      "dispatch",
      "workspace",
      "1"
    ]
  },
  {
    "label": "Workspace 2",
    "pattern": "^(?=.*\\bworkspace\\b)(?=.*\\b(two|2)\\b).*$",
    "argv": [
      "hyprctl",
      "dispatch",
      "workspace",
      "2"
    ]
  },
  {
    "label": "Volume Up",
    "pattern": "^(volume up|louder|turn (the )?volume up|increase volume|raise volume)$",
    "argv": [
      "pamixer",
      "-i",
      "5"
    ]
  },
  {
    "label": "Volume Down",
    "pattern": "^(volume down|lower|turn (the )?volume down|decrease volume|reduce volume)$",
    "argv": [
      "pamixer",
      "-d",
      "5"
    ]
  },
  {
    "label": "Lock Screen",
    "pattern": "^(lock( screen)?|.*\\block\\b.*)$",
    "argv": [
      "loginctl",
      "lock-session"
    ]
  }
]
